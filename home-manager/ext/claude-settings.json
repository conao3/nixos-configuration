{
  "theme": "dark",
  "defaultMode": "acceptEdits",
  "permissions": {
    "allow": [
      "WebFetch",
      "WebSearch",
      "mcp__chrome-devtools",
      "Bash(ag:*)",
      "Bash(awk:*)",
      "Bash(bb:*)",
      "Bash(cat:*)",
      "Bash(clj-kondo:*)",
      "Bash(clj:*)",
      "Bash(date:*)",
      "Bash(diff:*)",
      "Bash(echo:*)",
      "Bash(env:*)",
      "Bash(file:*)",
      "Bash(find:*)",
      "Bash(git branch:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git status:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(less:*)",
      "Bash(ls:*)",
      "Bash(make:*)",
      "Bash(mkdir:*)",
      "Bash(more:*)",
      "Bash(npm run:*)",
      "Bash(npx shadow-cljs:*)",
      "Bash(printenv:*)",
      "Bash(pwd:*)",
      "Bash(rg:*)",
      "Bash(sed:*)",
      "Bash(sort:*)",
      "Bash(stat:*)",
      "Bash(tail:*)",
      "Bash(tree:*)",
      "Bash(uniq:*)",
      "Bash(wc:*)",
      "Bash(which:*)",
      "Bash(whoami:*)"
    ],
    "deny": []
  },
  "env": {
    "CLAUDE_CODE_DISABLE_FEEDBACK_SURVEY": "1",
    "CLAUDE_CODE_ENABLE_TELEMETRY": "0",
    "DISABLE_ERROR_REPORTING": "1",
    "DISABLE_TELEMETRY": "1",
    "CDK_DISABLE_CLI_TELEMETRY": "true",
    "SAM_CLI_TELEMETRY": "0",
    "BASH_DEFAULT_TIMEOUT_MS": "300000",
    "BASH_MAX_TIMEOUT_MS": "1200000"
  },
  "preferredNotifChannel": "terminal_bell"
}
